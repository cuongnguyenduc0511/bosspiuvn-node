    "debug": "~2.6.9",
    "decode-html": "^2.0.0",
    "dotenv": "^6.2.0",
    "engines": "^0.4.0",
    "express": "~4.16.0",
    "express-handlebars-sections": "^1.0.1",
    "express-jwt": "^5.3.1",
    "express-sanitizer": "^1.0.4",
    "express-session": "^1.15.6",
    "file-system": "^2.2.2",
    "hbs": "~4.0.1",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "method-override": "^2.3.10",
    "moment": "^2.24.0",
    "mongoose": "^5.3.10",
    "morgan": "~1.9.0",
