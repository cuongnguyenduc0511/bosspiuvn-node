{
  "name": "boss-piuvn",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p start-server watch-css",
    "start-server": "nodemon ./server/bin/www",
    "build-css": "node-sass-chokidar src/ -o src/",
